{
  "name": "aeroplatzi",
  "version": 2,
  "builds": [{ "src": "index.js", "use": "@now/node" }],
  "routes": [{ "src": "/(.*)", "dest": "/index.js" }],
  "env": {
    "DB_USER": "@aeroplatzi-db-user",
    "DB_PASSWORD": "@aeroplatzi-db-password",
    "DB_HOST": "@aeroplatzi-db-host",
    "DB_NAME": "@aeroplatzi-db-name"
  }
}
